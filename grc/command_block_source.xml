<block>
  <name>Command source</name>
  <key>command_block_source</key>
  <category>Command_block</category>
  <import>import command_block</import>
  <make>command_block.command(0, $out_type.size, $cmd, 1, $blocking)</make>
    <param>
        <name>Output Type</name>
        <key>out_type</key>
        <value>float</value>
        <type>enum</type>
        <option>
            <name>Complex</name>
            <key>complex</key>
            <opt>size:gr.sizeof_gr_complex</opt>
        </option>
        <option>
            <name>Float</name>
            <key>float</key>
            <opt>size:gr.sizeof_float</opt>
        </option>
        <option>
            <name>Int</name>
            <key>int</key>
            <opt>size:gr.sizeof_int</opt>
        </option>
        <option>
            <name>Short</name>
            <key>short</key>
            <opt>size:gr.sizeof_short</opt>
        </option>
        <option>
            <name>Byte</name>
            <key>byte</key>
            <opt>size:gr.sizeof_char</opt>
        </option>
    </param>
  <param>
    <name>command</name>
    <key>cmd</key>
    <type>string</type>
  </param>
  <param>
    <name>Blocking IO</name>
    <key>blocking</key>
    <value>0</value>
    <type>enum</type>
    <option>
      <name>none</name>
      <key>0</key>
    </option>
    <option>
      <name>out</name>
      <key>2</key>
    </option>
  </param>
  <source>
    <name>out</name>
    <type>$out_type</type>
    <nports>1</nports>
  </source>
</block>
